# NodeSpace Desktop App Configuration
# Copy this file to .env and customize for your environment

# Database Configuration
# Development: Use e2e_sample.db for testing with sample data
# Production: Use production.db with real user data
NODESPACE_DB_PATH=../../data/lance_db/e2e_sample.db

# Models Configuration  
# Path to ONNX models directory containing gemma-3-1b-it-onnx/
NODESPACE_MODELS_PATH=../../models

# Environment Mode (development, testing, production)
NODE_ENV=development

# Logging Configuration
RUST_LOG=info,app_lib=debug

# Optional: Override specific model file
# NODESPACE_MODEL_FILE=../../models/gemma-3-1b-it-onnx/model.onnx